{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "develop": "yarn --cwd packages/frontend/app develop",
    "start": "yarn --cwd packages/frontend/app start",
    "build": "yarn --cwd packages/frontend/app build",
    "deploy:frontend": "yarn --cwd packages/frontend/app deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint . --ext js,jsx,mdx",
    "lint:fix": "yarn lint --fix",
    "cypress:open": "yarn --cwd packages/frontend/e2e cypress:open",
    "cypress:run": "yarn --cwd packages/frontend/e2e cypress:run",
    "test": "yarn --cwd packages/design-system test",
    "postinstall": "husky install",
    "plop": "plop"
  },
  "workspaces": [
    "packages/frontend/app",
    "packages/frontend/e2e",
    "packages/design-system",
    "packages/shared"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@svgr/webpack": "^6.4.0",
    "cross-env": "7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-check-file": "^1.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "npm-run-all": "4.1.5",
    "plop": "^3.1.2",
    "prettier": "^2.7.1",
    "wait-on": "7.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@storybook/addon-controls": "^6.5.12",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.15",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/theming": "^6.5.16",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "swiper": "^9.1.0",
    "usehooks-ts": "^2.9.1"
  }
}
